{
    "manifest_version": 2,
    "name": "Sandbox Extension",
    "version": "0.0",
    "content_scripts": [
        {
            "matches": [
                "http://cnn.com/*",
                "https://cnn.com/*"
            ],
            "js": ["content.js"]
        }

    ],
    "background": {
        "js": "background.js"
    }



}