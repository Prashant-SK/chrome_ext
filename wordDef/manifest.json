{
    "manifest_version": 2,
    "name": "WordDef",
    "version": "0.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }

    ],
    "permissions":["tabs"],
    "browser_action": {
        "default_icon": "open-peeps.png",
        "default_popup": "popup.html",
        "default_title": "WordDefinition"
    }



}