{
    "manifest_version": 2,
    "name": "Popup",
    "version": "0.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.ndtv.com/*"
            ],
            "js": ["content.js"]
        }

    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "open-peeps.png",
        "default_popup": "popup.html",
        "default_title": "Popup!"
    }



}